<h1>RailsBridgeCorp Open Jobs</h1>

<% @jobs.each do |job| %> <!-- Accidentally including the `=` will append the function's return to the view -->
	<h3><%= job.title %></h3>
	<p><%= job.description %></p>
	<h5><%= link_to "Edit Posting", edit_job_path(job) %></h5> <h5><%= link_to 'Delete Posting', job, method: :delete %></h5>
<% end %>